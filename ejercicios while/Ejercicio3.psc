Proceso Ejercicio3
	Definir suma, numero como Entero;
    
    suma <- 0;
    numero <- 1;
    
    Mientras numero <= 100 Hacer
        suma <- suma + numero * numero;
        numero <- numero + 1;
    FinMientras
    
    Escribir "La suma de los cuadrados de los números entre 1 y 100 es:", suma;
	
FinProceso
